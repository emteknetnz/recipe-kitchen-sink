{
    "name": "silverstripe/recipe-kitchen-sink",
    "type": "silverstripe-recipe",
    "description": "Internal testing Silverstripe CMS recipe for all modules, optional and suggested",
    "homepage": "https://github.com/silverstripe/recipe-kitchen-sink",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.1",
        "silverstripe/recipe-plugin": "2.x-dev",
        "silverstripe/installer": "5.x-dev",
        "silverstripe/recipe-authoring-tools": "2.x-dev",
        "silverstripe/recipe-blog": "2.x-dev",
        "silverstripe/recipe-collaboration": "2.x-dev",
        "silverstripe/recipe-form-building": "2.x-dev",
        "silverstripe/recipe-reporting-tools": "2.x-dev",
        "silverstripe/recipe-services": "2.x-dev",
        "silverstripe/recipe-content-blocks": "3.x-dev",
        "silverstripe/recipe-solr-search": "3.x-dev",
        "silverstripe/recipe-ccl": "3.x-dev",
        "silverstripe/developer-docs": "5.x-dev",
        "silverstripe/registry": "3.x-dev",
        "silverstripe/totp-authenticator": "5.x-dev",
        "silverstripe/mfa": "5.x-dev",
        "silverstripe/crontask": "3.x-dev",
        "silverstripe/gridfieldqueuedexport": "3.x-dev",
        "silverstripe/ldap": "2.x-dev",
        "silverstripe/textextraction": "4.x-dev",
        "silverstripe/realme": "5.x-dev",
        "silverstripe/ckan-registry": "2.x-dev",
        "silverstripe/webauthn-authenticator": "5.x-dev",
        "silverstripe/security-extensions": "5.x-dev",
        "silverstripe/subsites": "dev-pulls/3/param-order as 3.x-dev",
        "cwp/starter-theme": "4.x-dev",
        "cwp/agency-extensions": "3.x-dev",
        "cwp/watea-theme": "4.x-dev",
        "cwp/cwp": "3.x-dev",
        "dnadesign/silverstripe-elemental-userforms": "4.x-dev",
        "symbiote/silverstripe-multivaluefield": "6.x-dev",
        "silverstripe/silverstripe-fluent": "7.x-dev",
        "silverstripe/asset-admin": "dev-pulls/2/broken-builds as 2.x-dev",
        "silverstripe/frameworktest": "1.x-dev",
        "silverstripe/versioned": "dev-pulls/2/broken-builds as 2.x-dev",
        "dnadesign/silverstripe-elemental": "dev-pulls/5/broken-builds as 5.x-dev",
        "silverstripe/userforms": "dev-pulls/6/broken-builds as 6.x-dev",
        "silverstripe/cms": "dev-pulls/5/broken-builds as 5.x-dev",
        "silverstripe/comments": "dev-pulls/4/param-order as 4.x-dev",
        "silverstripe/lumberjack": "dev-pulls/3/param-order as 3.x-dev",
        "cwp/cwp-core": "dev-pulls/3/broken-builds as 3.x-dev"
    },
    "require-dev": {
        "silverstripe/frameworktest": "^1",
        "silverstripe/graphql-devtools": "1.x-dev",
        "silverstripe/recipe-testing": "^3",
        "mikey179/vfsstream": "^1.6"
    },
    "extra": {
        "project-files": [
            "app/*"
        ],
        "public-files": [
            "assets/*",
            "favicon.ico"
        ],
        "project-files-installed": [
            ".env.example",
            ".graphql-generated/_manifest_exclude",
            ".htaccess",
            "app/.htaccess",
            "app/_config.php",
            "app/_config/assets.yml",
            "app/_config/blog.yml",
            "app/_config/contentblocks.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/_config/theme.yml",
            "app/src/BlocksPage.php",
            "behat.yml",
            "phpunit.xml.dist"
        ],
        "public-files-installed": [
            ".htaccess",
            "assets/.gitignore",
            "assets/.htaccess",
            "assets/web.config",
            "favicon.ico",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "repositories": {
        "x0": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-asset-admin"
        },
        "x1": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-versioned"
        },
        "x2": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-elemental"
        },
        "x3": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-userforms"
        },
        "x4": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-cms"
        },
        "x5": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-comments"
        },
        "x6": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-lumberjack"
        },
        "x7": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-subsites"
        },
        "x8": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/cwp-core"
        }
    }
}
