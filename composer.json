        "silverstripe/iframe": "dev-pulls/3/php82 as 3.x-dev",
        "silverstripe/documentconverter": "dev-pulls/3/php82 as 3.x-dev",
        "silverstripe/solr-php-client": "dev-pulls/master/php82 as v1.0.2",
        "symbiote/silverstripe-queuedjobs": "dev-pulls/5/admin-email as 5.x-dev",
        "silverstripe/fulltextsearch": "dev-pulls/4/php82 as 4.x-dev"
    },
    "require-dev": {
        "silverstripe/frameworktest": "^1",
        "silverstripe/graphql-devtools": "1.x-dev",
        "silverstripe/recipe-testing": "^3",
        "mikey179/vfsstream": "^1.6"
    },
    "extra": {
        "project-files": [
            "app/*"
        ],
        "public-files": [
            "assets/*",
            "favicon.ico"
        ],
        "project-files-installed": [
            ".env.example",
            ".graphql-generated/_manifest_exclude",
            ".htaccess",
            "app/.htaccess",
            "app/_config.php",
            "app/_config/assets.yml",
            "app/_config/blog.yml",
            "app/_config/contentblocks.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/_config/theme.yml",
            "app/src/BlocksPage.php",
            "behat.yml",
            "phpunit.xml.dist"
        ],
        "public-files-installed": [
            ".htaccess",
            "assets/.gitignore",
            "assets/.htaccess",
            "assets/web.config",
            "favicon.ico",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        },
        "platform": {
            "php": "8.2.0"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "repositories": {
        "x3": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-userforms"
        },
