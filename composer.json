        "silverstripe/versioned": "dev-pulls/3/remove-graphql as 3.x-dev",
        "silverstripe/framework": "dev-pulls/6/remove-graphql as 6.x-dev",
        "silverstripe/recipe-cms": "dev-pulls/6/remove-graphql as 6.x-dev"
    },
    "extra": {
        "project-files": [
            "app/*"
        ],
        "public-files": [
            "assets/*",
            "favicon.ico"
        ]
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        },
        "platform": {
            "php": "8.1"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "repositories": {
        "x0": {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-admin"
        },
        "x1": {
